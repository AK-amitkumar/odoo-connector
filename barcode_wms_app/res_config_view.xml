<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
	<data>
		<record id="view_stock_config_settings_app" model="ir.ui.view">
			<field name="name">Stock Settings</field>
			<field name="model">stock.config.settings</field>
			<field name="inherit_id" ref="stock.view_stock_config_settings"/>
			<field name="arch" type="xml">
				<xpath expr="//group[last()]" position="after">
					<separator string="APP"/>
					<group>
						<label for="id" string="Scan"/>
						<div>
							<div>
								<field name="app_scan_material" class="oe_inline"/>
								<label for="app_scan_material"/>
							</div>
						</div>
					</group>
				</xpath>
			</field>
		</record>
	</data>
</openerp>
